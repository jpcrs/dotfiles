{
    //VSCODE CONFIG
    "editor.accessibilitySupport": "off",
    "editor.cursorSurroundingLines": 15,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.startupEditor": "newUntitledFile",
    "editor.stickyTabStops": true,
    "editor.suggestSelection": "first",
    "zenMode.fullScreen": false,
    "zenMode.hideTabs": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "workbench.layoutControl.enabled": false,
    "workbench.layoutControl.type": "menu",
    "workbench.editor.tabSizing": "shrink",
    "explorer.autoReveal": false,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.preferredDarkColorTheme": "Gruvbox Material Dark",
    "workbench.preferredHighContrastLightColorTheme": "Gruvbox Material Light",
    "workbench.preferredLightColorTheme": "Gruvbox Material Light",
    "workbench.preferredHighContrastColorTheme": "Gruvbox Material Dark",
    "workbench.activityBar.visible": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
        "vscodevim.vim": 1
    },
    //VIM STUFF
    "vim.vimrc.path": "$HOME/.config/vscode/vsvim.vim",
    "vim.vimrc.enable": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": false,
    "vim.incsearch": true,
    "vim.easymotion":true,
    "vim.leader": ",",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        },
        {
            "before": ["<c-j>"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before": ["u"],
            "commands": [
                "undo"
            ]
        },
        {
            "before": ["U"],
            "commands": [
                "redo"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        }
    ],
