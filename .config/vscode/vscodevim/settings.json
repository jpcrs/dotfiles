{
    // Vim
    "vim.easymotion": true,
    "vim.hlsearch": false,
    "vim.incsearch": true,
    "vim.leader": ",",
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.config/vscode/vscodevim/vsvim.vim",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                " "
            ],
            "commands": [
                "whichkey.show"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "U"
            ],
            "commands": [
                "redo"
            ]
        },
        //Commands that moves the cursor and can't be defined in the vimrc file.
        {
            "before": [
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "<C-w>",
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinitionAside"
            ]
        },
        {
            "before": [
                "g",
                "f"
            ],
            "commands": [
                "editor.action.revealDeclaration"
            ]
        },
        {
            "before": [
                "g",
                "L"
            ],
            "commands": [
                "workbench.action.navigateToLastEditLocation"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "p"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "n"
            ],
            "commands": [
                "goToNextReference"
            ]
        },
        {
            "before": [
                "g",
                "N"
            ],
            "commands": [
                "goToPreviousReference"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "R"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        }
    ],
    // Whichkey
