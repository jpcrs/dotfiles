            "key": ";",
            "name": "Toggle Side Bar",
            "icon": "layout-sidebar-left",
            "type": "conditional",
            "bindings": [
                {
                    "key": "",
                    "name": "Show Sidebar",
                    "type": "command",
                    "command": "workbench.action.focusSideBar"
                },
                {
                    "key": "when:sideBarVisible",
                    "name": "Hide Sidebar",
                    "type": "command",
                    "command": "workbench.action.closeSidebar"
                }
            ],
        },
        {
            "key": "j",
            "name": "Jump to a character",
            "type": "commands",
            "icon": "live-share",
