{
    "editor.stickyTabStops": true,
    "window.menuBarVisibility": "toggle",
    "editor.suggestSelection": "first",
    "vim.useSystemClipboard": true,
    "editor.lineNumbers": "relative",
    "editor.cursorSurroundingLines": 12,
    "editor.renderWhitespace": "boundary",
    "vim.leader": " ",
    "workbench.startupEditor": "newUntitledFile",
    "security.workspace.trust.untrustedFiles": "open",
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.config/vim/.codevimrc",
    "workbench.editor.untitled.hint": "hidden",
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1,
        "asvetliakov.vscode-neovim": 1
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        // {
        //     "before": ["<leader>", "d"],
        //     "commands": [ "editor.action.addSelectionToNextFindMatch" ]
        // },
        {
            "before": [ "<leader>", "D" ],
            "commands": [ "editor.action.selectHighlights" ]
        },
        {
            "before": [ "<leader>", "l", "e" ],
            "commands": [ "editor.action.marker.next" ]
        },
        {
            "before": [ "<leader>", "l", "E" ],
            "commands": [ "editor.action.marker.prev" ]
        },
        {
            "before": [ "<leader>", "l", "i"],
            "commands": [ "editor.action.marker.nextInFiles" ]
        },
        {
            "before": [ "<leader>", "l", "I"],
            "commands": [ "editor.action.marker.prevInFiles" ]
        },
        {
            "before": [ "<space>" ],
            "commands": [ "whichkey.show" ]
        }
        // {
        //     "before": ["<leader>", "v", "v"],
        //     "commands": [ "editor.action.smartSelect.expand" ]
        // }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ "<C-d>" ],
            "commands": [ "editor.action.addSelectionToNextFindMatch" ]
        },
        // {
        //     "before": ["<leader>", "d"],
        //     "commands": [ "editor.action.addSelectionToNextFindMatch"]
        // },
        {
            "before": [ "<leader>", "D" ],
            "commands": [ "editor.action.selectHighlights" ]
        },
        {
            "before": [ "<space>" ],
            "commands": [ "whichkey.show" ]
        }
        // {
        //     "before": ["<leader>", "v", "v"],
        //     "commands": [ "editor.action.smartSelect.expand" ]
        // }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [ "<C-d>" ],
            "commands": [ "editor.action.addSelectionToNextFindMatch" ]
        },
        {
            "before": [ "<C-d>" ],
            "commands": [ "editor.action.selectHighlights" ]
        },
    ],
    "editor.columnSelection": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/home/jpcrs/bin/nvim",
    "errorLens.enabledDiagnosticLevels": [
        "warning",
        "info",
        "error"
    ],
    "errorLens.enabled": true,
    "whichkey.bindings": [
        {
            "key": "l",
            "name": "LSP...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "r",
                    "name": "Rename",
                    "type": "command",
                    "command": "editor.action.rename"
                },
                {
                    "key": "a",
                    "name": "Actions",
                    "type": "command",
                    "command": "editor.action.quickFix"
                },
                {
                    "key": "m",
                    "name": "Method Signature",
                    "type": "command",
                    "command": "editor.action.triggerParameterHints"
                },
                {
                    "key": "h",
                    "name": "Hover Help",
                    "type": "command",
                    "command": "editor.action.showDefinitionPreviewHover"
                },
                {
                    "key": "s",
                    "name": "Search Symbols",
                    "type": "command",
                    "command": "workbench.action.gotoSymbol"
                },
                {
                    "key": "S",
                    "name": "Show All Symbols",
                    "type": "command",
                    "command": "workbench.action.showAllSymbols"
                },
                {
                    "key": "p",
                    "name": "Problems",
                    "type": "command",
                    "command": "editor.action.showDefinitionPreviewHover"
                },
                {
                    "key": "e",
                    "name": "Error (Next)",
                    "type": "command",
                    "command": "editor.action.marker.next"
                },
                {
                    "key": "E",
                    "name": "Error (Previous)",
                    "type": "command",
                    "command": "editor.action.marker.prev"
                },
                {
                    "key": "i",
                    "name": "Solution Error",
                    "type": "command",
                    "command": "editor.action.marker.nextInFiles"
                },
                {
                    "key": "I",
                    "name": "Solution Error (Previous)",
                    "type": "command",
                    "command": "editor.action.marker.prevInFiles"
                }
            ]
        },
        {
            "key": "s",
            "name": "Search...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "r",
                    "name": "Recent",
                    "type": "command",
                    "command": "workbench.action.openRecent"
                },
                {
                    "key": "f",
                    "name": "File",
                    "type": "command",
                    "command": "workbench.action.quickOpen"
                },
                {
                    "key": "e",
                    "name": "Everything",
                    "type": "command",
                    "command": "workbench.action.openRecent"
                },
                {
                    "key": "a",
                    "name": "All Commands",
                    "type": "command",
                    "command": "workbench.action.openRecent"
                }
            ]
        },
        {
            "key": "g",
            "name": "Git...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "B",
                    "name": "Blame",
                    "type": "command",
                    "command": "gitlens.toggleFileBlame"
                },
                {
                    "key": "d",
                    "name": "Diff Against Previous",
                    "type": "command",
                    "command": "gitlens.diffWithPrevious"
                },
                {
                    "key": "b",
                    "name": "Branch Change",
                    "type": "command",
                    "command": "gitlens.views.switchToAnotherBranch"
                },
                {
                    "key": "c",
                    "name": "Compare Working with Branch",
                    "type": "command",
                    "command": "gitlens.compareWokingWith"
                },
                {
                    "key": "w",
                    "name": "Worktrees",
                    "type": "command",
                    "command": "gitlens.gitCommands.worktree"
                }
            ]
        },
        {
            "key": "b",
            "name": "Breakpoints...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "b",
                    "name": "Breakpoint Toggle",
                    "type": "command",
                    "command": "editor.debug.action.toggleBreakpoint"
                },
                {
                    "key": "d",
                    "name": "Delete All Breakpoints",
                    "type": "command",
                    "command": "workbench.debug.viewlet.action.removeAllBreakpoints"
                },
                {
                    "key": "D",
                    "name": "Disable All Breakpoints",
                    "type": "command",
                    "command": "workbench.debug.viewlet.action.disableAllBreakpoints"
                },
                {
                    "key": "e",
                    "name": "Enable All Breakpoints",
                    "type": "command",
                    "command": "workbench.debug.viewlet.action.enableAllBreakpoints"
                }
            ]
        },
        {
            "key": "s",
            "name": "Start...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "D",
                    "name": "Debug Application",
                    "type": "command",
                    "command": "workbench.action.debug.start"
                },
                {
                    "key": "d",
                    "name": "Run Application (Without Debugging)",
                    "type": "command",
                    "command": "workbench.action.debug.run"
                },
                {
                    "key": "b",
                    "name": "Build",
                    "type": "command",
                    "command": "workbench.action.tasks.build"
                }
            ]
        },
        {
            "key": "t",
            "name": "Test...",
            "type": "bindings",
            "bindings": [
            ]
        },
        {
            "key": "f",
            "name": "File...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "e",
                    "name": "Show File Explorer",
                    "type": "command",
                    "command": "workbench.view.explorer"
                }
            ]
        },
        {
            "key": "L",
            "name": "Leetcode...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "s",
                    "name": "Search Problem",
                    "type": "command",
                    "command": "leetcode.searchProblem"
                },
                {
                    "key": "p",
                    "name": "Show Problem",
                    "type": "command",
                    "command": "leetcode.showProblem"
                },
                {
                    "key": "h",
                    "name": "Show Solution",
                    "type": "command",
                    "command": "leetcode.showSolution"
                },
                {
                    "key": "t",
                    "name": "Test",
                    "type": "command",
                    "command": "leetcode.testSolution"
                },
                {
                    "key": "v",
                    "name": "View Panel",
                    "type": "command",
                    "command": "leetCodeExplorer.focus"
                }
            ]
        },
        {

            "key": "w",
            "name": "Workspace...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "a",
                    "name": "Add Folder To Workspace",
                    "type": "command",
                    "command": "workbench.action.addRootFolder"
                },
                {
                    "key": "c",
                    "name": "Change Workspace Folder",
                    "type": "command",
                    "command": "workbench.action.files.openFolderViaWorkspace"
                },
                {
                    "key": "r",
                    "name": "Remove Folder from Workspace",
                    "type": "command",
                    "command": "workbench.action.removeRootFolder"
                },
                {
                    "key": "C",
                    "name": "Close Workspace",
                    "type": "command",
                    "command": "workbench.action.closeFolder"
                }
            ]
        }
    ]
}